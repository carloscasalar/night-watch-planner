{
  "name": "night-watch-planner",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel public/index.html ",
    "build": "cross-env NODE_ENV=production parcel build public/index.html --no-minify --public-url /"
  },
  "dependencies": {
    "autoprefixer": "^7.2.1",
    "babel-preset-react-app": "^3.1.0",
    "parcel-bundler": "latest",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "devDependencies": {
    "cross-env": "^5.1.1",
    "less": "^2.7.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
